<td class="col-lg-10 col-md-10 col-sm-10"><small><%= object.name.nil? ? "unknown" : object.name %></small></td>
<td class="col-lg-2 col-md-10 col-sm-10 actions">
<%if isManager %>	
	<%= smart_listing_item_actions [
		{name: :custom,    url: edit_brand_path(object),
	  		method: :get, 
  			icon: 'fa fa-pencil-square-o', 
  			title: "Edit"}]%>
<%end%>
<%if isAdmin %>
<%= smart_listing_item_actions [
  			{name: :destroy, url: brand_path(object), title: "Destroy",
			confirmation: "Are you sure you want to delete this?",
			placement:"left"}]%>
<%end%>

 </td>
