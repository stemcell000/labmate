  <table class="table table-hover">
    <thead>
	      <tr>
	        <th class="col-lg-10 col-sm-10 col-md-10" ><%= smart_listing.sortable t('global.name'), :name %></th>
			<th class="col-lg-2 col-sm-2 col-md-2"></th>
	      </tr>
    </thead>
    <tbody>
    	<% unless smart_listing.empty? %>  
      <% smart_listing.collection.each do |provider| %>
        <tr class="editable" data-id = "<%= provider.id %>" >
         	<%= smart_listing.render object: provider, partial: "providers/smart_listing/provider", locals: {object: provider} %>
        </tr>
      	<% end %>
	<% end %>
    </tbody>
  </table>
  <%= smart_listing.paginate %>
  <%= smart_listing.pagination_per_page_links %>


