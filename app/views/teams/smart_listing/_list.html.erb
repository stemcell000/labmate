	  <table  class="table table-striped table-small">
	    <thead>
	    		<th class="col-lg-3 center">
		        <%= smart_listing.sortable "Acronym", :acronym %>
		    </th>
		    <th class="col-lg-3 center">
		        <%= smart_listing.sortable "Name", :name %>
		    </th>
		    <th class="col-lg-3 center">
		        <%= smart_listing.sortable "Department", :department_name %>
		    </th>
		    <th class="col-lg-1 center">
		        <%= smart_listing.sortable "Type", :type_name %>
		    </th>
		  	<th class="col-lg-2 right"></th>
	    </thead>
	    <tbody>
		  	<% smart_listing.collection.each do |p| %>
	      		<tr class="editable" data-id = "<%= p.id %>">
					<%= smart_listing.render object: p, partial: "teams/smart_listing/team", locals: {object: p} %>
	        		</tr>
	      	<% end %>
	     <%= smart_listing.item_new colspan: 9, link: new_team_path(:object) %>
	    </tbody>
	  </table>
	  <%= smart_listing.paginate %>
	  <%= smart_listing.pagination_per_page_links %>
