	  <table  class="table table-striped table-small">
	    <thead>
		    <th class="col-lg-2 col-md-3 col-sm-3 center">
		        <%= sort_link(@q, :lastname, 'Name', default_order: :desc) %>
		    </th>
		    <th class="col-lg-2 col-md-2 col-sm-2 center"> e-mail
		    </th>
		    <th class="col-lg-2  col-md-2 col-sm-2 center"> telephone
		    </th>
		    <th class="col-lg-2  col-md-2 col-sm-2 center"> Location
		    </th>
		    <th class="col-lg-3 col-md-3 col-sm-3 center">
		   	 	<%= sort_link(@q, 'teams_name', 'Team', default_order: :desc) %>
		    </th>
		  	<th class="col-lg-1 center"">
		  	</th>
	    </thead>
	    <tbody>
		  <% unless smart_listing.empty? %> 
		  	<% smart_listing.collection.each do |p| %>
	      		<tr class="editable" data-id = "<%= p.id %>">
					<%= smart_listing.render object: p, partial: "users/smart_listing/user", locals: {object: p} %>
	        		</tr>
	      	<% end %>
	      	<%end%>
	    </tbody>
	  </table>
	  <%= smart_listing.paginate %>
	  <%= smart_listing.pagination_per_page_links %>
