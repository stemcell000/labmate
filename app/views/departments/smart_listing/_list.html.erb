	  <table  class="table table-striped">
	    <thead>
	      <tr>
	        <th class="col-lg-10 col-sm-10 col-md-10" ><%= sort_link(@q, :name, t('menu.name'), default_order: :desc) %></th>
			<th class="col-lg-2 col-sm-2 col-md-2"></th>
	      </tr>
	    </thead>
	    <tbody>
	    	<% unless smart_listing.empty? %>  
		  	<% smart_listing.collection.each do |p| %>
	      		<tr class="editable" data-id = "<%= p.id %>">
					<%= smart_listing.render object: p, partial: "departments/smart_listing/department", locals: {object: p} %>
	        		</tr>
	      	<% end %>
	     <%end%>
	    </tbody>
	  </table>
	  <%= smart_listing.paginate %>
	  <%= smart_listing.pagination_per_page_links %>
